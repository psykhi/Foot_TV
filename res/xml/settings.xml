            <CheckBoxPreference
                android:defaultValue="true"
                android:key="canalplus"
                android:title="Canal +" >
            </CheckBoxPreference>
            <CheckBoxPreference
                android:defaultValue="true"
                android:key="canalsport"
                android:title="Canal + Sport" >
                </CheckBoxPreference>
            <CheckBoxPreference
                android:defaultValue="true"
