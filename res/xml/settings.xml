    </PreferenceCategory>
    <PreferenceCategory android:title="Feedback" >
        <Preference
            android:key="devFeedback"
            android:summary="Faites part de vos bugs/suggestions au développeur"
            android:title="Ecrire un mail au développeur" >
            <intent
                android:action="android.intent.action.VIEW"
                android:data="@string/contact_developer_uri" />
        </Preference>
    </PreferenceCategory>
    <PreferenceCategory android:title="Appli Premium" >
        <Preference
            android:key="removeAds"
            android:summary="Enlever toutes les publicités de l&apos;application"
            android:title="Enlever les pubs!" >
        </Preference>
    </PreferenceCategory>

</PreferenceScreen>